{
    "name": "docteurklein/event-store",
    "description": "An event store",
    "license": "MIT",
    "authors": [
        {
            "name": "Klein Florian",
            "email": "florian.klein@free.fr"
        }
    ],
    "require": {
        "php": ">=5.5",
        "doctrine/common": "~2.4",
        "jms/serializer": "~0.16.0"
    },
    "require-dev": {
        "phpspec/phpspec": "*@dev",
        "docteurklein/funk-spec": "*@dev",
        "rhumsaa/uuid": "~2.7.1",
        "symfony/debug": "~2.5",
        "bossa/phpspec2-expect": "*@dev",
        "doctrine/orm": "~2.4"
    },
    "autoload": {
        "psr-0": {
            "Knp\\Event": "src",
            "PhpSpec": "src",
            "TestWork\\PhpSpecExtension": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "example": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
